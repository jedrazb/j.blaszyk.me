{
  "name": "j.blaszyk.me",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "description": "Jedr Blaszyk - Personal Blog",
  "version": "1.0.0",
  "author": "Jedrze Blaszyk <j@blaszyk.com>",
  "bugs": {
    "url": "https://github.com/jedrazb/personal-blog"
  },
  "dependencies": {
    "gatsby": "^4.5.3",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-catch-links": "^4.5.0",
    "gatsby-plugin-feed": "^4.5.0",
    "gatsby-plugin-google-analytics": "^4.5.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^4.5.0",
    "gatsby-plugin-offline": "^5.5.2",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-sharp": "4.5.2",
    "gatsby-plugin-sitemap": "^5.5.0",
    "gatsby-plugin-typography": "^4.5.0",
    "gatsby-remark-autolink-headers": "5.5.0",
    "gatsby-remark-component-parent2div": "^1.2.3",
    "gatsby-remark-copy-linked-files": "^5.5.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^6.5.2",
    "gatsby-remark-katex": "^6.5.0",
    "gatsby-remark-mathjax": "^1.0.0",
    "gatsby-remark-prismjs": "^6.5.0",
    "gatsby-remark-responsive-iframe": "^5.5.0",
    "gatsby-remark-smartypants": "^5.5.0",
    "gatsby-source-filesystem": "^4.5.2",
    "gatsby-transformer-remark": "^5.5.2",
    "gatsby-transformer-sharp": "^4.5.0",
    "husky": "^1.3.1",
    "katex": "^0.11.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "rehype-react": "^7.0.4",
    "swiper": "^7.4.1",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.3",
    "prettier": "^1.14.2"
  },
  "homepage": "https://j.blaszyk.me",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "now-build": "gatsby build",
    "deploy": "yarn build && cp now.json public/ && cp CNAME public/ && gh-pages -d public",
    "dry": "yarn build && cp now.json public/ && cd public && now",
    "js-to-jsx": "node src/utils/js-to-jsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ],
    "src/pages/**/*.md": [
      "yarn run js-to-jsx",
      "git add"
    ]
  },
  "resolutions": {
    "**/sharp": "^0.29.3"
  }
}
